PRO GKV_FixGS2_data, GS2Structure;; Accepts the structure which NetCDF_Data produces from; from a GS2 NetCDF file as input.  It corrects the ; independent variables;;  Written by W.M. Nevins;     8/26/03;; First, check for the tag 't';TagNames = TAG_NAMES(GS2Structure)nTags = N_TAGS(GS2Structure)FOR it=0,nTags-1 DO BEGIN	IF(STRCMP('t', TagNames[it], /FOLD_CASE)) THEN GOTO, GOT_itENDFORGOTO, DONE_itGOT_it :; ; Get pointer to t values;GS2Structure.t -> get, values=tPtr, units=tUnitstValues = *tPtrtMin = MIN(tValues, MAX=tMax)tRange=[tMin, tMax];; Get mnemonic which has been used for 't';GS2Structure.t -> get, axis=1, GridMnemonic=tName;; Now go through each object in GS2structure and put time values (from tPtr); into appropriate grid structure;FOR i=0,nTags-1 DO BEGIN	tAxis = GS2Structure.(i) -> AxisNumber(tName)	IF(Query_Integer(tAxis)) THEN BEGIN		GS2Structure.(i) -> GET, axis=tAxis, GridValues=old_tPtr		PTR_FREE, old_tPtr		new_tPtr = PTR_NEW(tValues)		GS2Structure.(i) -> SET, axis=tAxis, 		$					GridValues=new_tPtr, 	$					Range=tRange,		$					Uniform=0,		$					GridUnits=tUnits, 	$					GridMnemonic='t',	$					 GridTitle='t'	ENDIFENDFORDONE_it :;; Now, check for units embedded into the axis mnemonics;FOR i=0,NTags-1 DO BEGIN	kxAxis = GS2Structure.(i) -> AxisNumber('kx')	IF(Query_Integer(kxAxis)) THEN GS2Structure.(i) -> set, axis=kxAxis, GridTitle='k!Dx!N', GridUnits='!4q!X!Ds!N'	kyAxis = GS2Structure.(i) -> AxisNumber('ky')	IF(Query_Integer(kyAxis)) THEN GS2Structure.(i) -> set, axis=kyAxis, GridTitle='k!Dy!N', GridUnits='!4q!X!Ds!N'ENDFORRETURNEND